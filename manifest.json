{
  "manifest_version": 3,
  "name": "Basic Extension",
  "version": "1.0",
  "description": "Basic Extension",
  "host_permissions": ["https://github.com/*/*/pull/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*/pull/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
